<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>show.bind</Title>
    <Shortcut>au-showBind</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Markup snippet for a show.bind attribute</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>binding</ID>
        <ToolTip>binding</ToolTip>
        <Default>binding</Default>
      </Literal>
    </Declarations>
    <Code Language="html" Key="*"><![CDATA[show.bind="$binding$"$end$]]></Code>
  </Snippet>
</CodeSnippet>