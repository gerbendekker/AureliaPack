<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>replace-part</Title>
    <Shortcut>replace-part</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Markup snippet for a replace-part template element</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
      <SnippetType>SurroundsWith</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>item</ID>
        <ToolTip>item</ToolTip>
        <Default>item</Default>
      </Literal>
    </Declarations>
    <Code Language="html" Key="*">
      <![CDATA[<template replace-part="$item$">
  $selected$$end$
</template>]]>
    </Code>
  </Snippet>
</CodeSnippet>